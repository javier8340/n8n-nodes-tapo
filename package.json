{
  "name": "n8n-nodes-tapo",
  "version": "1.0.1",
  "description": "Adds Tapo nodes to n8n",
  "author": {
    "name": "JavierG",
    "email": "jgg83402@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "tapo",
    "tp-link",
    "iot",
    "smart-home"
  ],
  "homepage": "https://github.com/javier8340/n8n-nodes-tapo#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/javier8340/n8n-nodes-tapo.git"
  },
  "bugs": {
    "url": "https://github.com/javier8340/n8n-nodes-tapo/issues"
  },
  "main": "dist/TapoNode.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/TapoNode.js"
    ]
  },
  "dependencies": {
    "n8n-workflow": "^1.82.0",
    "tp-link-tapo-connect": "^2.0.7"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^5.3.3",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "prettier": "^3.5.3"
  },
  "engines": {
    "node": ">=18.10"
  },
  "files": [
    "dist",
    "resources"
  ],
  "packageManager": "npm@>=9"
}
